                                          {"before":["<leader>", "h"],"commands":[":tabfirst"]},
                                          {"before":["<leader>", "w","w"],"commands":[":w"]},
                                          {"before":["<leader>", "w","q"],"commands":[":wq"]},
                                          {"before":["<leader>", "q","q"],"commands":[":q"]}
                                          ],
