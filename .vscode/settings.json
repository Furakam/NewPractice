{
  "git.ignoreLimitWarning": true,
  "editor.fontFamily": "'Cascadia Code'",

  "vim.leader": "<space>",
  "vim.normalModeKeyBindings": [{"before":["<leader>", "k"],"commands":[":tabnext"]},
                                          {"before":["<leader>", "j"],"commands":[":tabprevious"]},
